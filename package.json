{
  "name": "lightchain-aio",
  "version": "1.0.0",
  "private": true,
  "description": "AI-Powered Block Explorer & Launchpad",
  "main": "src/index.tsx",
  "engines": { "node": ">=18" },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "workbox injectManifest workbox-config.js || true",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "analyze": "source-map-explorer 'build/static/js/*.js' --tsv > build/bundle-sizes.tsv || true"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.34.0",
    "axios": "^1.4.0",
    "ethers": "^5.8.0",
    "i18next": "^23.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "tailwindcss": "^3.5.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.6",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^2.8.8",
    "postcss": "^8.4.24",
    "source-map-explorer": "^2.6.2",
    "webpack-bundle-analyzer": "^4.7.0",
    "workbox-build": "^6.6.2",
    "workbox-cli": "^6.6.2"
  },
  "license": "MIT"
}
