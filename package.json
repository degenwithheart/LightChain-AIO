{
  "name": "lightchain-aio",
  "version": "1.0.0",
  "description": "AI-Powered Block Explorer & Launchpad",
  "main": "src/index.tsx",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "workbox injectManifest workbox-config.js",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "analyze": "source-map-explorer 'build/static/js/*.js' --tsv > build/bundle-sizes.tsv",
    "bundle:view": "webpack-bundle-analyzer build/static/js/*.js --no-open"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "ethers": "^5.8.0",
    "i18next": "^21.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.4",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "web3": "^1.8.0",
    "@tanstack/react-query": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "postcss": "^8.4.16",
    "source-map-explorer": "^2.6.2",
    "webpack-bundle-analyzer": "^4.6.1",
    "workbox-build": "^6.6.2",
    "workbox-cli": "^6.6.2"
  },
  "keywords": [
    "blockchain",
    "explorer",
    "launchpad",
    "AI",
    "web3"
  ],
  "author": "Degen Serenade",
  "license": "MIT"
}
