{
  "name": "lightchain-aio",
  "version": "1.0.0",
  "description": "AI-Powered Block Explorer & Launchpad",
  "main": "src/index.tsx",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' || true",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}' || true",
    "analyze": "npx source-map-explorer 'build/static/js/*.js' --tsv > build/bundle-sizes.tsv || true",
    "bundle:view": "npx webpack-bundle-analyzer build/static/js/*.js --no-open || true"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web3": "^1.8.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^4.9.5",
    "i18next": "^21.6.3",
    "react-i18next": "^11.15.3",
    "react-toastify": "^8.1.0",
    "axios": "^0.24.0",
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0",
    "source-map-explorer": "^2.5.2",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "keywords": [
    "blockchain",
    "explorer",
    "launchpad",
    "AI",
    "web3"
  ],
  "author": "Degen Serenade",
  "license": "MIT"
}
